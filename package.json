{
  "name": "next-portfolio",
  "description": "My Portfolio Site",
  "author": {
    "email": "coreyburns@outlook.com",
    "name": "Corey Burns",
    "url": "https://coreyburns.dev"
  },
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://https://github.com/burnsco/next-portfolio.git"
  },
  "license": "MIT",
  "keywords": [
    "gatsby",
    "portfolio",
    "react",
    "graphql",
    "full-stack",
    "emotion",
    "chakra-ui",
    "theme-ui",
    "styled-system"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build-export": "next build && next export",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.2",
    "@chakra-ui/react": "^1.1.0",
    "@chakra-ui/theme-tools": "1.0.2",
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@iconify/core": "^1.0.0-rc.4",
    "@iconify/icons-brandico": "^1.1.0",
    "@iconify/icons-cib": "^1.1.0",
    "@iconify/icons-clarity": "^1.1.0",
    "@iconify/icons-el": "^1.1.0",
    "@iconify/icons-entypo": "^1.1.0",
    "@iconify/icons-fa-brands": "^1.1.0",
    "@iconify/icons-heroicons-outline": "^1.1.0",
    "@iconify/icons-logos": "^1.1.0",
    "@iconify/icons-ls": "^1.1.0",
    "@iconify/icons-mdi": "^1.1.0",
    "@iconify/icons-simple-icons": "^1.1.0",
    "@iconify/icons-whh": "^1.1.0",
    "@iconify/react": "^2.0.0-rc.3",
    "@next/bundle-analyzer": "^10.0.4",
    "formik": "^2.2.6",
    "framer-motion": "^3.1.1",
    "next": "latest",
    "next-compose-plugins": "^2.2.1",
    "next-seo": "^4.17.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-headroom": "^3.1.0",
    "react-remove-scroll": "^2.4.1",
    "smoothscroll-polyfill": "^0.4.4",
    "webpack": "^5.11.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-headroom": "^2.2.1",
    "@types/sharp": "^0.26.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "next-unused": "^0.0.3",
    "prettier": "^2.2.1",
    "typescript": "4.1.3"
  },
  "next-unused": {
    "alias": {
      "@common/*": [
        "components/common/*"
      ],
      "@sections/*": [
        "components/sections/*"
      ],
      "@ui/*": [
        "components/ui/*"
      ],
      "@configs/*": [
        "configs/*"
      ],
      "@pages/*": [
        "pages/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "configs",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "resolutions": {
    "webpack": "^5.0.0-beta.30"
  }
}
